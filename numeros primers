def numerosPrimers(n):
    if n == 1:
        return False
    for x in range(2, n):
        if n % x == 0:
            return False
    else:
        return True

def primers(n = 1):
    while(True):
        if numerosPrimers(n): yield n
        n += 1

for n in primers():
    if n > 10: break
    print(n)
